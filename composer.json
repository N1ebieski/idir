{
    "name": "n1ebieski/idir",
    "description": "iDir",
    "keywords": ["iDir"],
    "type": "package",
    "require": {
        "php": "^7.3",
        "spatie/laravel-directory-cleanup": "^1.4",
        "n1ebieski/icore": "^2.0.0",
        "gusapi/gusapi": "^5.1",
        "league/omnipay": "^3.0",
        "omnipay/paypal": "^3.0",
        "mike182uk/paypal-ipn-listener": "^9.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mmo/faker-images": "^0.3",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "authors": [
        {
            "name": "Mariusz Wysoki≈Ñski N1ebieski",
            "email": "kontakt@intelekt.net.pl",
            "homepage": "https://github.com/N1ebieski",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "N1ebieski\\IDir\\": "src"
        }
